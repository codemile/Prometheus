/**
 * Example of inheritance.
 */
object Child {
	// an example  property
	this.Message = "Child: No!!!!!";
	
	// an example member function
	this.foo = function() {
		print this.Message;
	};
	
	print "Child: How are you?";
};

Child Father {
	base();
	print "Father: Luke I am your father";
	//this.Message = "Father: Surprised?";
};

var x = new Father();
x.foo();